body{
  font:15px/1.5 Arial, Helvetica, sans-serif;
  padding: 0;
  margin: 0;
  background-color: white;
}
.dark{
  background-color: black;
  padding: 15px;
  color: white;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);
  border-radius: 5px;
}

.white{
  background-color: white;
  padding: 15px;
  margin-top: 10px;
  margin-bottom: 10px;
  box-shadow: 0 0 15px 1px rgba(0, 0, 0, 0.2);
  border-radius: 5px;
}

.grey{
  background-color:#e6e6e6;
  padding: 10px;
  box-shadow: 0 0 15px 1px rgba(0, 0, 0, 0.2);
  border-radius: 5px;
}
.p_title{
  font-size: 15px;
  padding: 0px;
}
