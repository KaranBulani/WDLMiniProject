.topnav {
  overflow: hidden;
  background-color: #333;
  box-shadow: 0 0 15px 1px rgba(0, 0, 0, 0.4);
}

.topnav a {
  float: left;
  font-weight: bold;
  color: #f2f2f2;
  text-align: center;
  padding: 12px 30px;
  text-decoration: none;
  font-size: 17px;
  box-shadow: 0 0 15px 1px rgba(0, 0, 0, 0.2);
}

.topnav a:hover {
  background-color: #ddd;
  color: black;
  box-shadow: 0 0 15px 1px rgba(0, 0, 0, 0.2);
}

.topnav a.active {
  background-color: #99b3ff;
}
